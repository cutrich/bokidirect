create table booki_tbl(
id INT NOT NULL,
booki_id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
booki_employee VARCHAR(30) NOT NULL,
booki_cash INT(10) NOT NULL,
booki_card INT(10) NOT NULL,
booki_tip INT(10) NOT NULL,
booki_supply INT(10) NOT NULL,
bookie_date INT(10) NOT NULL,
created_date DATE
);

